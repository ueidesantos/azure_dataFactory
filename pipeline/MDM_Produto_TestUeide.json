								},
								"sink": {
									"name": "product_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['EAN']"
								},
								"sink": {
									"name": "ean",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['NOME']"
								},
								"sink": {
									"name": "Nome",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['APRESENTACAO']"
								},
								"sink": {
									"name": "Apresentacao",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['data']"
					}
				},
				"inputs": [
					{
						"referenceName": "ds_GuiaFarmacia",
						"type": "DatasetReference"
					}
				],
				"outputs": [
